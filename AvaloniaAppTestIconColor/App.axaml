<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:AvaloniaAppTestIconColor"
             x:Class="AvaloniaAppTestIconColor.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Resources>
      <ResourceDictionary>
        <SolidColorBrush x:Key="ColorIconsSecond"  Color="#7ac0ff"/>
        <ResourceDictionary.MergedDictionaries>
          <ResourceInclude Source="/Icons/ResourceDictionary1.axaml"/>
        </ResourceDictionary.MergedDictionaries>
      </ResourceDictionary>
    </Application.Resources>

    <Application.Styles>
        <FluentTheme Mode="Light"/>
    </Application.Styles>
</Application>
